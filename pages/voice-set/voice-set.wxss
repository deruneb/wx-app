.container button{
  width: 600rpx;
  height: 100rpx;
  font-size: 36rpx;
  text-align: center;
  line-height: 66rpx;
  color: #ffffff;
  margin: 0 auto 30rpx;
  background-color: #24D0C6;
}
.voice-add{
  text-align: center;
}
.voice-add>view:first-child{
  font-size: 30rpx;
  color: #ffffff;
  padding: 74rpx 0 44rpx;
}
.voice-add>view:nth-of-type(2){
  width: 530rpx;
  font-size: 24rpx;
  color: #ffffff;
  margin: 0 auto 210rpx;
}
.voice-add>view:nth-of-type(3){
  height: 160rpx;
  font-size: 100rpx;
  color: #ffffff;
  margin-bottom: 100rpx;
}
.voice-edit > view:first-child{
  padding: 244rpx 0 180rpx;
}
.voice-edit image{
  width: 262rpx;
  height: 262rpx;
  text-align: center;
  margin-bottom: 34rpx;
}