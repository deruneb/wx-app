/* pages/create-canvas/create-canvas.wxss */
.btn{
  width: 750rpx;
  font-size: 30rpx;
  color: #fff;
  text-align: center;
  margin-top: 100rpx;
}
/* 本地报表生成 */
.body_box {
  position: absolute;
  top: -2000rpx;
  left: 15rpx;
  width: 720rpx;
  height: 900rpx;
  background: #fff;
  border-radius: 5rpx;
  box-shadow: 0 0 10px 2px #ccc;
  z-index: -2;
  visibility: hidden;
}

.cover_box,
.cover_box1 {
  position: absolute;
  width: 100%;
  height: 100%;
}

.canvas_cover {
  width: 100%;
  height: 100%;
  border-radius: 5rpx;
}

.canvas_cover1{
  width: 100%;
  height: 100%;
  border-radius: 5rpx;
}
