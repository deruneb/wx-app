.flex {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}

.justify-content-between {
  justify-content: space-between;
}

.tips{
  font-size: 22rpx;
  color: #B1BFCD;
}

ec-canvas {
  width: 100%;
  height: 100%;
}

.content_boxs{
  padding: 20rpx 0rpx;
  background: #ffffff;
  margin-bottom: 20rpx;
}

.boxs_cont_info{
  width: calc(100vw - 80rpx);
  margin: 10rpx 40rpx;
}

.left-title-all{
  width:calc(100vw - 45rpx );
  height: 40rpx;
  padding:0rpx 20rpx;
  border-left: 5rpx solid #24D0C6;
  line-height: 40rpx;
  font-size: 26rpx;
  color: #3C4A55;
}